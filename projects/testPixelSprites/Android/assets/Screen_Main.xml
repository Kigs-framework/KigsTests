					<Attr N="Priority" V="3"/>
					<Attr N="Dock" V="{0.5,0.5}"/>
					<Attr N="Anchor" V="{0.5,0.5}"/>
					<Attr N="PreScaleX" V="16.0"/>
					<Attr N="PreScaleY" V="16.0"/>
					<Attr N="Repeat_U" V="true"/>
					<Attr N="Repeat_V" V="true"/>
					<Attr N="ForceNearest" V="false"/>
					
