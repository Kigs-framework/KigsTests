				<Attr N="Color" V="{0.0, 0.2, 0.4}"/>
			</Inst>
			
			<Inst N="tst" T="UIItem">
				<Attr N="Dock" V="{0.5,0.5}"/>
				<Attr N="Anchor" V="{0.5,0.5}"/>
